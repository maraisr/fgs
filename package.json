{
	"name": "fgs",
	"version": "0.1.1",
	"bin": "bin/cli.js",
	"files": [
		"bin"
	],
	"scripts": {
		"build": "esbuild src/cli.ts --analyze --format=cjs --platform=node --target=node14 --outdir=bin",
		"format": "prettier  --write --list-different \"{*,src/**/*,.github/**/*}.+(ts|json|yml|md)\"",
		"prepublishOnly": "pnpm run build",
		"typecheck": "tsc --noEmit",
		"watch": "pnpm run build -- --watch"
	},
	"exports": {
		"./package.json": "./package.json"
	},
	"dependencies": {
		"graphql-config": "^4.1.0",
		"httpie": "^1.1.2",
		"kleur": "^4.1.4",
		"sade": "^1.8.1",
		"swrt": "^0.0.6"
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.2",
		"esbuild": "^0.14.32",
		"graphql": "^16.3.0",
		"prettier": "^2.6.2",
		"typescript": "^4.6.3"
	},
	"peerDependencies": {
		"graphql": ">= 15"
	},
	"engines": {
		"node": ">=14"
	},
	"volta": {
		"node": "14.19.1"
	}
}
