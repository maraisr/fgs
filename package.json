{
	"name": "fgs",
	"version": "0.1.0-alpha.1",
	"bin": "bin/cli.js",
	"files": [
		"bin"
	],
	"scripts": {
		"build": "esbuild cli.ts --banner:js=\"#!/usr/bin/env node\" --format=cjs --platform=node --target=node12 --outdir=bin --bundle --external:globby --external:sade --external:httpie --external:graphql-config --external:kleur --external:klona --external:swrt --external:graphql",
		"format": "prettier  --write --list-different \"{*,example/**/*,.github/**/*}.+(ts|json|yml|md)\"",
		"prepublishOnly": "pnpm run build",
		"typecheck": "tsc --noEmit",
		"watch": "pnpm run build -- --watch"
	},
	"dependencies": {
		"globby": "11.1.0",
		"graphql-config": "^4.1.0",
		"httpie": "^1.1.2",
		"kleur": "^4.1.4",
		"klona": "^2.0.5",
		"sade": "^1.8.1",
		"swrt": "^0.0.6"
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.2",
		"esbuild": "^0.14.11",
		"graphql": "^16.2.0",
		"prettier": "^2.5.1",
		"typescript": "^4.5.4"
	},
	"peerDependencies": {
		"graphql": ">= 15"
	},
	"volta": {
		"node": "16.0.0"
	}
}