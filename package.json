{
	"name": "fgs",
	"version": "0.0.6",
	"bin": "bin/cli.js",
	"files": [
		"bin"
	],
	"scripts": {
		"build": "esbuild cli.ts --banner:js=\"#!/usr/bin/env node\" --format=cjs --platform=node --target=node12 --outdir=bin",
		"format": "prettier  --write --list-different \"{*,example/**/*,.github/**/*}.+(ts|json|yml|md)\"",
		"prepublishOnly": "pnpm run build",
		"typecheck": "tsc --noEmit",
		"watch": "pnpm run build -- --watch"
	},
	"dependencies": {
		"diary": "^0.2.2",
		"graphql-config": "^4.0.2",
		"httpie": "^1.1.2",
		"kleur": "^4.1.4",
		"mri": "^1.2.0",
		"swrt": "^0.0.6"
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.1",
		"esbuild": "^0.13.5",
		"graphql": "^15.6.1",
		"prettier": "^2.4.1",
		"typescript": "^4.4.4"
	},
	"peerDependencies": {
		"graphql": ">= 15"
	},
	"volta": {
		"node": "16.0.0"
	}
}
